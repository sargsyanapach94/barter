      "type": "string",
      "id":2,
      "required": true
    },
    "user_id": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string"
    },
    "district": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "city": {
      "type": "string"
    }
  },
  "validations": [],
