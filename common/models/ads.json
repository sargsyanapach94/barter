{
  "name": "ad",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "message": {
      "type": "string"
    },
    "category_key": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "user_id": {
      "type": "string",
      "required": true
    },
    "user_name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "image": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
      "users": {
        "type": "belongsTo",
        "model": "user",
        "foreignKey": "user_id"
      },
      "contacts": {
        "type": "belongsTo",
        "model": "contact",
        "foreignKey": "email"
      }
    },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "custom roles",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
